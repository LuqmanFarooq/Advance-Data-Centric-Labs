<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	
	<bean id="engine1.2" class="Engine">
		<constructor-arg name="size" value="1.2" ></constructor-arg>
		<constructor-arg name="fuel" value="Petrol" ></constructor-arg>
	</bean>
	<bean id="engine1.6" class="Engine">
		<constructor-arg name="size" value="1.6"></constructor-arg>
		<constructor-arg name="fuel" value="Petrol"></constructor-arg>
	</bean>
</beans>
